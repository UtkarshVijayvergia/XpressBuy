  "version": "1.0.0",
  "description": "E-commerce website using AWS services",
  "main": "server.js",
  "scripts": {
    "start": "node backend/server.js",
    "server": "nodemon --legacy-watch backend/server.js",
    "client": "npm start --prefix frontend"
